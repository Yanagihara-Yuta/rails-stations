<h1>Admin::Movies#index</h1>
<p>Find me in app/views/admin/movies/index.html.erb</p>
<table>
<tr>
<th>ID</th>
<th>映画タイトル</th>
<th>公開年</th>
<th>上映中か</th>
<th>概要</th>
<th>画像URL</th>
<th>作成日時</th>
<th>更新日時</th>
<th></th>
</tr>
    <% @movies.each do | movie | %>
        
    <tr>
        <td><%= movie.id %></td>
        <td><%= movie.name %></td>
        <td><%= movie.year %></td>
        <td>
            <% if movie.is_showing == true %>
                上映中
            <% else %>
                上映予定
            <% end %>
        </td>
        <td><%= movie.description %></td>
        <td><%= link_to movie.image_url, movie.image_url %></td>
        <td><%= movie.created_at %></td>
        <td><%= movie.updated_at %></td>
        <td></td>
        </tr>
    <% end %>
</table>